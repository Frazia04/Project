{
  "name": "exclaim",
  "version": "0.2.0-SNAPSHOT",
  "private": "true",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "npm-run-all --continue-on-error check:typecheck-node check:typecheck-app check:prettier check:stylelint check:eslint",
    "check:typecheck-node": "vue-tsc --noEmit -p tsconfig.node.json --composite false",
    "check:typecheck-app": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "check:prettier": "prettier --log-level warn --check .",
    "check:stylelint": "stylelint --ignore-path .gitignore '**/*.scss'",
    "check:eslint": "eslint --ignore-path .gitignore --ext .vue,.ts,.js,.cjs .",
    "lint": "npm-run-all --continue-on-error lint:prettier lint:stylelint lint:eslint lint:prettier",
    "lint:prettier": "prettier --log-level warn --write .",
    "lint:stylelint": "stylelint --ignore-path .gitignore --fix '**/*.scss'",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .vue,.ts,.js,.cjs --fix .",
    "browserslist": "browserslist"
  },
  "dependencies": {
    "@fontsource-variable/red-hat-mono": "^5.0.19",
    "@fontsource-variable/red-hat-text": "^5.0.19",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@material-symbols/svg-400": "^0.17.4",
    "jquery": "^3.7.1",
    "v-tooltip": "^2.1.3",
    "vue": "^3.4.26",
    "vue-router": "^4.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/types": "^7.24.5",
    "@faker-js/faker": "^8.4.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "browserslist": "^4.23.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^24.0.0",
    "miragejs": "^0.1.48",
    "node-storage-shim": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "tsx": "^4.9.0",
    "typescript": "~5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-minify": "^1.5.2",
    "vue-tsc": "^2.0.16",
    "xlsx": "^0.18.5"
  }
}
